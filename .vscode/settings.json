{
  "cSpell.words": [
    "changefreq",
    "easyops",
    "flowbite",
    "httpsnippet",
    "noreply",
    "peaceiris",
    "savechart",
    "sharechart"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.quickSuggestions": {
    "strings": "inline",
    "comments": "inline"
  },
  "[markdown]": {
    "editor.wordWrap": "off",
    "editor.quickSuggestions": {
      "other": "on",
      "comments": "inline",
      "strings": "inline"
    }
  },
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": ".github/workflows/deploy.yml"
  },
  "tailwindCSS.experimental.classRegex": [
    "classed\\(.+\\)`(.*)`",
    "classed\\.\\w+`(.*)`"
  ]
}
