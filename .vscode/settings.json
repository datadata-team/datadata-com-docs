{
  "cSpell.words": [
    "altzone",
    "asctime",
    "changefreq",
    "copysign",
    "crossorigin",
    "dataframe",
    "ddof",
    "dtype",
    "easyops",
    "flowbite",
    "httpsnippet",
    "iterrows",
    "katex",
    "localtime",
    "noreply",
    "peaceiris",
    "Resampler",
    "savechart",
    "sharechart",
    "tzname"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "[markdown]": {
    "editor.wordWrap": "off",
  },
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": ".github/workflows/deploy.yml"
  },
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass"
  ],
  "tailwindCSS.experimental.classRegex": [
    "classed\\(.+\\)`(.*)`",
    "classed\\.\\w+`(.*)`"
  ]
}
